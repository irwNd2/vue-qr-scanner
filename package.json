{
  "name": "vue-qr-scanner",
  "version": "0.1.5",
  "description": "Vue 3 QR/Barcode scanner with overlay + fallback pipeline (BarcodeDetector â†’ ZXing WASM)",
  "repository": {
    "type": "git",
    "url": "https://github.com/irwNd2/vue-qr-scanner.git"
  },
  "homepage": "https://github.com/irwNd2/vue-qr-scanner#readme",
  "type": "module",
  "main": "dist/vue-qr-scanner.umd.cjs",
  "module": "dist/vue-qr-scanner.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/vue-qr-scanner.es.js",
      "require": "./dist/vue-qr-scanner.umd.cjs",
      "default": "./dist/vue-qr-scanner.es.js"
    },
    "./style.css": "./dist/style.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vue-tsc -p tsconfig.build.json && vite build",
    "dev": "vite build --watch",
    "typecheck": "vue-tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "vue",
    "qr",
    "qrcode",
    "barcode",
    "scanner",
    "wasm",
    "zxing"
  ],
  "author": "Irwansyah",
  "license": "MIT",
  "peerDependencies": {
    "vue": ">=3.3.0"
  },
  "dependencies": {
    "zxing-wasm": "^2.2.1"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.4",
    "@vue/compiler-sfc": "^3.5.21",
    "typescript": "^5.5.0",
    "vite": "^5.4.0",
    "vue": "^3.4.0",
    "vue-tsc": "^2.0.20"
  }
}
